set size 1, 0.5
set locale "pt_BR"
set encoding iso_8859_1
set xlabel "Posição"
set ylabel "Velocidade"
set term postscript portrait
set term postscript color
set output "vtable-mcar-contour.eps"
set view 0, 0
set ytics 25
set xtics 14
set contour
set nosurface
splot [0:100] [0:100] "vtable-mcar.txt" matrix every 2:2 notitle with lines
